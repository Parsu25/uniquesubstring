#include<stdio.h>
#include<string.h>
int main() 
{
 char a[100];
 int l;
 scanf("%s",a);
 l=strlen(a);
 substring(a);
}
int substring(char a[])
{
    int l,i,j,k,c=1;
    l=strlen(a);
    printf("%d",l);
    for(i=0;i<l;i++)
    {
        for(j=i+1;j<l;j++)
        {
            if(a[i]==a[j])
            {
                a[j]=c++;
                break;
            }
            else if(j==(l-1)&&(a[j]!=a[i]))
            {
                printf("%c",a[i]);
            }
        }
    }
    if(isalpha(a[l-1]))
    {
        printf("%c",a[l-1]);
    }
}
